{
  "name": "gatsby-tachyons-interUI",
  "description": "Gatsby + Tachyons + InterUI",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "purgecss -c purgecss.config.js -o ./src/css && gatsby build",
    "serve": "gatsby serve",
    "css": "tachyons src/css/index.css > src/css/main.css --minify",
    "prestart": "npm run css",
    "prebuild": "npm run css"
  },
  "dependencies": {
    "gatsby": "^1.9.250",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "inter-ui": "^2.5.1",
    "purgecss": "^0.21.0",
    "react-helmet": "^5.2.0",
    "tachyons": "^4.9.1",
    "tachyons-cli": "^1.3.0"
  }
}
